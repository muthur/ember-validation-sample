<div role="form" class="form-horizontal">
    <div class="form-group">
        <label for="addressLine1" class="col-sm-4 control-label">Address Line1</label>
        <div class="col-sm-8">
          {{input type="text" id="addressLine1" class="form-control" value=addressLine1 placeholder="Address Line1"}}
            <div class="error" id="error-addressLine1"></div>
        </div>
    </div>
    <div class="form-group">
        <label for="addressLine2" class="col-sm-4 control-label">Address Line2</label>
        <div class="col-sm-8">
          {{input type="text" id="addressLine2" class="form-control" value=addressLine2 placeholder="Address Line2"}}
            <div class="error" id="error-addressLine2"></div>
        </div>
    </div>
    <div class="form-group">
        <label for="zipCode" class="col-sm-4 control-label">Zipcode</label>
        <div class="col-sm-8">
          {{input type="text" id="zipCode" class="form-control" value=zipCode placeholder="Zipcode"}}
            <div class="error" id="error-zipCode"></div>
        </div>
    </div>
</div>
